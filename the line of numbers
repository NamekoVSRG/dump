#include <iostream>
using namespace std;

int main() {
    int numbers[]{9,2,1,8,4,7,3,6,5,10,5,11,13,12,14};
    for (int i = 0; i < 15; ++i)
    {
        for (int e = 0;e < 15; ++e)
        {
            if(numbers[i] == numbers[e] && i == e)
            {
                break;
            }
            else if(numbers[i] == numbers[e] && i != e)
            {
                cout << "We have a match!" << i << " and " << e << endl;
                return 0;
            }

        }
    }
}
